<h1>Listing spieltags</h1>

<table>
  <tr>
    <th>Saison</th>
    <th>Nummer</th>
    <th>Datum</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @spieltags.each do |spieltag| %>
  <tr>
    <td><%= spieltag.saison %></td>
    <td><%= spieltag.nummer %></td>
    <td><%= spieltag.datum %></td>
    <td><%= link_to 'Show', spieltag %></td>
    <td><%= link_to 'Edit', edit_spieltag_path(spieltag) %></td>
    <td><%= link_to 'Destroy', spieltag, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

